{
    "presets": [
        ["env",
        {
            "modules": false,
            "targets":
            {
                "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
            }
        }],
        "stage-2"
    ],
    "plugins": ["transform-runtime", "transform-vue-jsx", "jsx-v-model"],
    "env":
    {
        "test":
        {
            "presets": ["env", "stage-2"],
            "plugins": ["istanbul"]
        }
    }
}